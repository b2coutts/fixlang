; list append

; add the given element to the end of a list
let addToEnd {2,
    if .1       cons car .1
                     .0 cdr .1 .2
    else        [.2]
}

; append 2 lists
let append {2,
    if .2       .0 addToEnd .1  car .2
                   cdr .2
    else        .1
}



append [1,2,3,4,5] [6,7,8,9]
