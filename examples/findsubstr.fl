# true iff .1 begins with .2
beginsWith: [
    if 2
        if 1
            if eq <1 <2         0 >1 >2
            else                .0
        else                    .0
    else                        .1
]

# true iff .2 is a substring of .1
isSubstr: [
    if          beginsWith 1 2          .1
    else if     1                       0 >1 2
    else                                .0
]

isSubstr "this is a test" "is a "
