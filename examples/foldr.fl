; foldr
let foldr {3, 
    if .3
        .1  car .3
            .0 .1. .2 cdr .3
    else
        .2
}

; really fucking terse
let terse {3, if .3 .1 car .3 .0 .1. .2 cdr .3 .2}

; create a list from .1 to .2; assumes .2>.1
let range (
    if - .1 .2          cons .1  .0 + .1 1 .2
    else                cons .1 empty
)

terse +. 0 range 1 1000000
